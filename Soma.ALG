Algoritmo "semnome"

Var

   soma, multiplicacao, menos, N, M : inteiro

Inicio

   escreva("Digite um número: ")
   leia(N)
   escreva("Digite outro número: ")
   leia(M)
   
   soma <- N + M
   
   multiplicacao <- N * M
   
   menos <- N - M
   
   escreval("Soma = ", soma)
   escreval("Multiplicação = ", multiplicacao)
   escreval("Menos = ", menos)
   

Fimalgoritmo